#{node1=Id("node")} {node0=node1-543}
group 'meshnodes' equals node {node0} to {node1}
#{numhexes=68}
#{_i=0}
#{loop(68)}
#{_hex0=Id("hex")}
#{_offset=node0+_i*8}
create hex node {_offset} to {_offset+7}
#{_hex1=Id("hex")}
#{If(_hex0==_hex1)}
#{_numfails++}
#{_failedhex=_hex1+_numfails}
echo on
Comment "Failed to create hex " {_failedhex} ". numfails=" {_numfails}
# create an error
list hex {_failedhex}
echo off
#{EndIf}
#{++_i}
#{endloop}
#{hex1=Id("hex")} {hex0=hex1-num_hexes-1}
group 'meshhexes' equals hex {hex0} to {hex1}
zoom reset
display
echo on
journal on
